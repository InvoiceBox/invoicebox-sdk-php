{
  "name": "invoicebox/sdk-php",
  "description": "This is a developer tool for integration with Invoicebox API",
  "type": "library",
  "license": "MIT",
  "homepage": "https://docs.invoicebox.ru/docs/sdk/php/",
  "keywords": ["invoicebox", "invbox", "payments", "api", "sdk"],
  "authors": [
    {
      "name": "Invoicebox Developers",
      "email": "developer@invbox.ru"
    }
  ],
  "dist": {
    "type": "zip",
    "url": "https://github.com/InvoiceBox/invoicebox-sdk-php/archive/refs/heads/master.zip"
  },
  "require": {
    "php": "^7.4|^8.1",
    "symfony/http-client-contracts": "^2.0|^3.0",
    "symfony/http-client": "^5.0|^6.0",
    "ext-curl": "*",
    "ext-json": "*"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.5",
    "phpstan/phpstan": "^1.8",
    "symplify/easy-coding-standard": "^11.0"
  },
  "autoload": {
    "psr-4": {
      "Invoicebox\\Sdk\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Invoicebox\\Sdk\\Tests\\": "tests/"
    }
  }
}
